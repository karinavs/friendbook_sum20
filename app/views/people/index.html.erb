<h1 class="ui header myHeader">People</h1>

<img class='myImg' src='https://images.unsplash.com/photo-1591107032477-8c72ea467b70?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=800&q=60'/>

<table class="ui celled table">
  <thead>
    <tr><th>First Name</th>
    <th>Last Name</th>
    <th>Phone</th>
    <th>Avatar</th>
    <th>Email</th>
  </tr>
  </thead>
  <tbody>
  <% @people.each do |person| %>
    <tr>
      <td><%= person.first_name %></td>
      <td><%= person.last_name %></td>
      <td><%= person.phone %></td>
      <td><%= person.avatar %></td>
      <td><%= person.email %></td>
    </tr>
  <% end %>
  </tbody>
</table>
